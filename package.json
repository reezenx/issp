  "scripts": {
    "start": "nx serve client",
    "start:api": "nx serve api",
    "start:client:prod": "nx serve client --prod",
    "start:api:prod": "nx serve api --prod",
    "start:prod": "npm run start:client:prod && start:api:prod",
    "e2e:portal": "nx e2e client-e2e --ui",
    "e2e:api": "nx e2e api-e2e",
    "serve": "nx run-many --target=serve --projects=api,client",
    "build": "nx run-many --target=build --projects=api,client",
    "build:prod": "nx run-many --target=build --projects=api,client --prod",
    "serve:prod": "nx run-many --target=serve --projects=api,client --prod",
    "db:studio": "prisma studio --schema apps/api/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:deploy": "npx prisma migrate deploy",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "prod:portal": "nx build client --prod",
    "prod:api": "nx build api --prod",
    "graph": "nx graph",
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "format": "nx format:write --all",
    "commit": "nx affected --target=build && cz-cust",
    "production:build": "npx prisma migrate deploy && npm run build:prod"
  },
