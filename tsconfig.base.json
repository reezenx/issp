{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2020",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@iisp/admin/data-access/dashboard": [
        "libs/admin/data-access/dashboard/src/index.ts"
      ],
      "@iisp/admin/data-access/projects": [
        "libs/admin/data-access/projects/src/index.ts"
      ],
      "@iisp/admin/data-access/users": [
        "libs/admin/data-access/users/src/index.ts"
      ],
      "@iisp/client/data-access/account": [
        "libs/client/data-access/account/src/index.ts"
      ],
      "@issp/shared/client/services": [
        "libs/shared/client/services/src/index.ts"
      ],
      "@issp/shared/constant": ["libs/shared/constant/src/index.ts"],
      "@issp/shared/data-access/auth": [
        "libs/shared/data-access/auth/src/index.ts"
      ],
      "@issp/shared/data-access/profile": [
        "libs/shared/data-access/profile/src/index.ts"
      ],
      "@issp/shared/data-access/security": [
        "libs/shared/data-access/security/src/index.ts"
      ],
      "@issp/shared/data-access/settings": [
        "libs/shared/data-access/settings/src/index.ts"
      ],
      "@issp/shared/prisma-client-main": [
        "libs/shared/prisma-client-main/src/index.ts"
      ],
      "@issp/shared/prisma-schema-main": [
        "libs/prisma-schema-main/src/index.ts"
      ],
      "@issp/shared/ui/layouts": ["libs/shared/ui/layouts/src/index.ts"],
      "@issp/shared/ui/libraries": ["libs/shared/ui/libraries/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
